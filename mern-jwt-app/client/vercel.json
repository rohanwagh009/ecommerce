{
  "version": 2,
  "framework": null,
  "buildCommand": "npm install && npm run build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "outputDirectory": "build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_VERSION": "18"
  }
}
